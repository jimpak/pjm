<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:key="notification"
        android:title="알림 설정">
        <SwitchPreferenceCompat
            key="noti_message"
            title="메세지 알림"/>
        <Preference
            android:key="feedback"
            android:title="피드백 보내기"
            android:summary="Report tecnical issues or suggest new features"/>
    </PreferenceCategory>
    <PreferenceCategory
        android:key="etc"
        android:title="기타 설정">
        <EditTextPreference
            key="id"
            title="ID 설정"/>
        <ListPreference
            android:entries="@array/my_color"
            android:entryValues="@array/my_color_values"
            android:key="color"
            android:title="색상 선택"/>
    </PreferenceCategory>

</PreferenceScreen>