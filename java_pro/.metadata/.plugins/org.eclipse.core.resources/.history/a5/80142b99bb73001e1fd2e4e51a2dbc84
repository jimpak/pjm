package practice5;

public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] scores = new int[3];
		scores[0] = 80;
		scores[1] = 70;
		scores[2] = 80;
		
		int sum = 0;
		int avg = 0;
		for(int scoreNum : scores) {
			sum += scoreNum;
			if(scoreNum == 2)
				avg = sum/scoreNum;
		}
		System.out.println(avg);
	}

}
