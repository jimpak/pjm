package practice6;

import java.util.Scanner;

public class 박정민 {
	public static void main(String[] args) {
		
		int x = 10;
		int y = 2;
		int z = 2;
		
		z=++x/y;
		System.out.println(x + " " + y + " " + z);
		x*=y+1;
		System.out.println(x + " " + y + " " + z);
		z=++x+y++;
		System.out.println(x + " " + y + " " + z);

	}
}

